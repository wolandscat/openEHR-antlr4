import org.splash-lang.foundation.representation.base_types ;


class Interval_event 
    is_a Event

feature_group("access")

    |
    | Duration of event.
    |
    prop width : Duration ;

    |
    | Information items captured in this Event
    |
    prop sample_count : Integer ;

    prop sample_function : Sample_function_kind ;

end




