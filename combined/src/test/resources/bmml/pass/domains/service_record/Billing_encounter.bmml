abstract class Billing_encounter
    is_a Care_process

feature_group("access")

    prop identifier : Rwe_id_ref ;

    prop external_identifiers :? Node [*; list] ;

    prop service_events :? Service_event [*; list] ;

    prop lifecycle_state : Encounter_state ;

    |
    | Mode of interaction: virtual, attendance, etc
    |
    prop interaction_mode : Terminology_term ;

end

