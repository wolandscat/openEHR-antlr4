enumeration Encounter_state
    base_type String

feature_group ("values")

    planned ("planned");

    in_progress ("in_progress");

    on_hold ("on_hold");

    completed ("completed");

    discharged ("discharged");

    abandoned ("abandoned");

    cancelled ("cancelled");

end


